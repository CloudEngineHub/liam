{
  "name": "@liam-hq/e2e",
  "private": true,
  "version": "0.0.0",
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@playwright/test": "1.50.1",
    "@types/node": "22.9.0",
    "typescript": "5"
  },
  "scripts": {
    "fmt": "pnpm run '/^fmt:.*/'",
    "fmt:biome": "biome check --write --unsafe .",
    "lint": "pnpm run '/^lint:.*/'",
    "lint:biome": "biome check .",
    "test:e2e": "playwright test tests/e2e",
    "test:vrt:compare": "playwright test tests/vrt",
    "test:vrt:screenshots": "playwright test tests/vrt --update-snapshots"
  }
}
