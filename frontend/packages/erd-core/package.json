  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@liam/db-structure": "workspace:*",
    "@packages/configs": "workspace:*",
    "@types/react": "^18",
    "typed-css-modules": "^0.9.1",
    "typescript": "^5"
