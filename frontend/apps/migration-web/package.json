{
  "name": "migration-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm gen && next build",
    "dev": "pnpm run '/^dev:.*/'",
    "dev:css": "tcm components --watch",
    "dev:next": "next dev --port 3003",
    "fmt": "pnpm run '/^fmt:.*/'",
    "fmt:biome": "biome check --write --unsafe .",
    "gen": "pnpm run '/^gen:.*/'",
    "gen:css": "tcm components",
    "lint": "pnpm run '/^lint:.*/'",
    "lint:biome": "biome check .",
    "start": "next start"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18",
    "next": "15.1.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@types/node": "22.9.0",
    "@types/react": "18",
    "@types/react-dom": "18",
    "typed-css-modules": "0.9.1",
    "typescript": "5"
  }
}
